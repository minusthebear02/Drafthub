<h1>My Teams</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>League</th>
    </tr>
  </thead>
  <tbody>
    <% @user.teams.each do |team| %>
      <tr>
        <td><%= link_to team.name, league_team_path(team.league, team) %></td>
        <td><%= link_to team.league.name, league_path(team.league) %></td>
      </tr>
    <% end %>
  </tbody>
</table>